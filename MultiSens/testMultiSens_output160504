#announce that the state is changing from 2 (STIM_PERIOD) to 4 (RESPONSE_WINDOW)
b'19136 ST_CHG 2 4\r\n'
b'19138 ST_CHG2 2 4\r\n'

#wait through the response window
b'20000 DBG\r\n'
b'21000 DBG\r\n'
b'22000 DBG\r\n'

#announce that the state is changing from 4 (RESPONSE_WINDOW) to 6 (INTER_TRIAL_INTERVAL)
b'22139 ST_CHG 4 6\r\n'
b'22139 ST_CHG2 4 6\r\n'

#announce trial results
b'22140 TRLR RESP 2\r\n'
b'22140 TRLR OUTC 3\r\n'

#wait through inter-trial interval
b'23000 DBG\r\n'
b'24000 DBG\r\n'
b'25000 DBG\r\n'

#announce that the state is changing from 6 (INTER_TRIAL_INTERVAL) to 0 (WAIT_TO_START_TRIAL)
b'25140 ST_CHG 6 0\r\n'
b'25140 ST_CHG2 6 0\r\n'

b'25489 ACK SET STPRIDX 0\n' #acknowledgement of trial 4, 1st parameter (indeed this was set to 0 in Python script) 
b'25499 ACK SET SPKRIDX 1\n' #acknowledgement of trial 4, 2st parameter (indeed this was set to 1 in Python script) 
b'25515 ACK RELEASE_TRL\n' #acknowledgement of release trial message
b'25515 TRL_RELEASED\r\n' #announce trial release

#announce that the state is changing from 0 (WAIT_TO_START_TRIAL) to 1 (TRIAL_START)
b'25515 ST_CHG 0 1\r\n'
b'25517 ST_CHG2 0 1\r\n'

#announce trial parameters
b'25517 TRL_START\r\n'
b'25517 TRLP STPRIDX 0\r\n'
b'25517 TRLP SPKRIDX 1\r\n'
b'25517 TRLP STIMDUR 2000\r\n'
b'25517 TRLP REW 0\r\n'

#announce that the state is changing from 1 (TRIAL_START) to 2 (STIM_PERIOD)
b'25517 ST_CHG 1 2\r\n'
b'25526 ST_CHG2 1 2\r\n'

#this time, only the speaker should be active. Indeed, this is what we see here:
