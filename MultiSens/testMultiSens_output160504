#announce that the state is changing from 2 (STIM_PERIOD) to 4 (RESPONSE_WINDOW)
b'27529 ST_CHG 2 4\r\n'
b'27531 ST_CHG2 2 4\r\n'

#wait through response window
b'28000 DBG\r\n'
b'29000 DBG\r\n'
b'30000 DBG\r\n'

#announce that the state is changing from 4 (RESPONSE_WINDOW) to 6 (INTER_TRIAL_INTERVAL)
b'30532 ST_CHG 4 6\r\n'
b'30532 ST_CHG2 4 6\r\n'

#announce trial results
b'30533 TRLR RESP 2\r\n'
b'30533 TRLR OUTC 3\r\n'

#wait through inter-trial interval
b'31000 DBG\r\n'
b'32000 DBG\r\n'
b'33000 DBG\r\n'

#announce that state is changing from 6 (INTER_TRIAL_INTERVAL) to 0 (WAIT_TO_START_TRIAL)
b'33533 ST_CHG 6 0\r\n'
b'33533 ST_CHG2 6 0\r\n'